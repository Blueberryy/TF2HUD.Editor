  "Controls": {
	"HUD Layout": [
		{
			"Name": "zh_val_layout_style",
			"Label": "Layout Style",
			"Type": "ComboBox",
			"Value": "0",
			"ToolTip": "Layout of health, ammo, objective, meters, etc. in-game.",
			"ComboFiles": [
				"addons//health-ammo-style_ut//",
				"addons//health-ammo-style_classic//"
			],
			"Options": [
				{
					"Label": "Default",
					"Value": "0"
				},
				{
					"Label": "Unreal Tournament (UT Mode)",
					"Value": "1",
					"FileName": "addons//health-ammo-style_ut//"
				},
				{
					"Label": "Classic",
					"Value": "2",
					"FileName": "addons//health-ammo-style_classic//"
				}
			]
		},
		{
			"Name": "zh_val_layout_font",
			"Label": "Font",
			"Type": "ComboBox",
			"Value": "0",
			"ToolTip": "Font used for Health, Ammo and Counters",
			"ComboFiles": [
				"addons//health-ammo_font_futura//"
			],
			"Options": [
				{
					"Label": "CIKANDEI (Default)",
					"Value": "0"
				},
				{
					"Label": "Futura Bold",
					"Value": "1",
					"FileName": "addons//health-ammo_font_futura//"
				}
			]
		}
    ],
